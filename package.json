  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "vite": "^7.3.1",
    "vite-plugin-vue-devtools": "^8.0.5"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
